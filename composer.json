{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "barryvdh/laravel-dompdf": "^1.0",
        "doctrine/dbal": "^3.3",
        "dyrynda/laravel-cascade-soft-deletes": "^4.1",
        "facade/ignition": "^2.3.6",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "igaster/laravel_cities": "^1.3",
        "laravel-json-api/laravel": "^2.x-dev",
        "laravel/cashier": "^13",
        "laravel/framework": "^v8.82.0",
        "laravel/sanctum": "^2.14",
        "laravel/tinker": "^v2.6.2",
        "laravel/ui": "^3.3",
        "owen-it/laravel-auditing": "^13.0",
        "picqer/php-barcode-generator": "^2.2",
        "ramsey/uuid": "^3.8|^4.2",
        "setasign/fpdi": "^2.3",
        "spatie/laravel-webhook-server": "^1.4",
        "symfony/filesystem": "^6.0",
        "tecnickcom/tcpdf": "^6.6",
        "torann/currency": "^1.1",
        "venturecraft/revisionable": "1.*",
        "webpatser/laravel-countries": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "fakerphp/faker": "^1.18",
        "filp/whoops": "^2.0",
        "kkomelin/laravel-translatable-string-exporter": "^1.16",
        "laravel-json-api/testing": "^1.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeds/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
